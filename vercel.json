{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/shorturl",
            "dest": "/index.js"
        },
        {
            "src": "/api/shorturl/:shortUrl",
            "dest": "/index.js"
        }
    ]
}